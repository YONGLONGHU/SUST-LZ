
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 20px;
}
.header {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-bottom: 20px; /* 添加间距 */
}
.title {
  font-size: 24px;
  color: #333;
  margin-bottom: 10px; /* 标题和按钮之间的间距 */
}
.btn {
  padding: 10px 20px; /* 按钮内边距 */
  background-color: #007aff; /* 按钮背景颜色 */
  color: white;
  border-radius: 5px; /* 按钮圆角 */
}
.data-item {
  margin-top: 20px;
  padding: 10px;
  background-color: #f0f0f0;
  border-radius: 5px;
}
.tem, .som {
  font-size: 18px;
  color: #333;
}
